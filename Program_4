// Write a C++ program to demonstrate order of constructor calls in Multiple Inheritance.

#include<iostream>
using namespace std;

class parent1{
	
	public:
			parent1()
			{
				   cout<<"My Name Is Bhalani Pal"<<endl;
    cout<<"En_no :: 220130318080 "<<endl;
				cout<<"This Is A Base Class Of Parent-1"<<endl;
			}
};
class parent2{
	
	public:
			Parent2()
			{
				cout<<"This Is A Base Class Of Parent-2"<<endl;
			}
};
class child : public parent1 , public parent2{
	
	public:
			child()
			{
				cout<<"The Base Class Is Child"<<endl;
			}
};
int main()
{
	child obj;
	return 0;
}
