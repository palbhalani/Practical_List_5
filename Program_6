// Write a C++ program to demonstrate Runtime polymorphism code without virtual keyword.

#include<iostream>
using namespace std;

class animal{
	
	public:
		void eat()
		{
				cout<<"Eating...:)"<<endl;
		}
};
class dog : public animal
{
	public:
		void eat()
		{
            cout<<"My Name Is Bhalani Pal"<<endl;
    cout<<"En_no :: 220130318080 "<<endl;
			cout<<"Dog Eating Bread...:"<<endl;		
		}	
};
int main()
{
	dog d=dog();
	d.eat();
	return 0;
}
