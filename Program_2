// Write a C++ program to demonstrate use of Multilevel Inheritance.

#include<iostream>
using namespace std;
class vehicle
{
  public :
          vehicle()
          {
             cout<<"My Name Is Bhalani Pal"<<endl;
    cout<<"En_no :: 220130318080 "<<endl;
            cout<<"This Is A Vehicle"<<endl;
          }
};
class forwheeler : public vehicel
{
  public :
          forwheeler()
          {
            cout<<"forwheeler Is A Vehicle";
          }
};
class car : public foreheeler 
{
  public :
      car()
      {
        cout<<"Car Is A Vehicel"<<endl;
      }
};
int main()
{
  car obj;
  return 0;
}
